{
  "name": "@rediskit/cache",
  "version": "1.1.3",
  "description": "A robust and type-safe Redis cache client built on ioredis, offering modern caching patterns and enterprise-grade reliability.",
  "author": "Mohammad Ibrahim",
  "homepage": "https://github.com/rediskit/cache#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rediskit/cache.git"
  },
  "bugs": {
    "url": "https://github.com/rediskit/cache/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run prettier && npm run lint",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:esm": "tsc --outDir dist/esm --module ESNext",
    "build:cjs": "tsc --outDir dist/cjs --module CommonJS",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest --config jest.config.js --ci --reporters=default --reporters=jest-junit --maxWorkers=2",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\""
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ioredis": "^5.6.1"
  },
  "peerDependencies": {
    "ioredis": "^5.6.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.6.0",
    "ioredis": "^5.6.1"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./test-reports",
    "outputName": "jest-junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "keywords": [
    "redis",
    "cache",
    "redis-cache",
    "redis-kit",
    "rediskit",
    "@rediskit",
    "@rediskit/cache",
    "@redis/cache",
    "awesomecoder.dev",
    "awesomecoder",
    "client"
  ]
}
